{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GEMINI_MODEL": "gemini-2.5-flash",
    "SERP_API_KEY": "@serp_api_key"
  }
}
